{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": ["airbnb"],
  "plugins": ["react"],
  "rules": {
     "no-param-reassign": 0,
	   "no-underscore-dangle": 0,
     "react/forbid-prop-types": 0,
     "react/require-default-props": 0,
     "object-curly-newline": 0,
     "react/prop-types": 0,
     "jsx-a11y/label-has-for": [ 2, {
      "required": {
          "every": ["id" ]
      }
  }]
   }
}